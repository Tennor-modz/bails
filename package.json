{
  "name": "@angstvorfrauen/baileys",
  "version": "1.0.0",
  "description": "Websocket Whatsapp API for Node.js",
  "keywords": [
    "whatsapp",
    "websocket",
    "api",
    "nodejs"
  ],
  "homepage": "https://github.com/angstvorfrauen/Baileys",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/angstvorfrauen/Baileys.git"
  },
  "license": "MIT",
  "author": "Xeuka",
  "main": "./lib/index.js",
  "files": [
    "lib/*",
    "WAProto/*.js",
    "WASignals/*.js"
  ],
  "scripts": {
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "node --inspect Example/example.js",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "lint": "eslint src --ext .js",
    "lint:fix": "yarn lint --fix",
    "release": "release-it",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^16.0.0",
    "@types/ws": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.0.0",
    "jest": "^27.0.6",
    "jimp": "^0.16.1",
    "json": "^11.0.0",
    "link-preview-js": "^3.0.0",
    "open": "^8.4.2",
    "protobufjs-cli": "^1.1.3",
    "release-it": "^15.10.3",
    "sharp": "^0.32.6"
  },
  "peerDependencies": {
    "audio-decode": "^2.1.3",
    "jimp": "^0.16.1",
    "link-preview-js": "^3.0.0",
    "sharp": "^0.32.6"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "bugs": {
    "url": "https://github.com/angstvorfrauen/Baileys/issues"
  }
}
